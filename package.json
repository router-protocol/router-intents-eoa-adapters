    "build": "npm run clean && npx hardhat compile",
    "test": "npx hardhat test",
    "lint": "prettier --write 'contracts/**/*.sol'",
    "postinstall": "husky install .husky",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
